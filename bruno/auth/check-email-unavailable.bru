meta {
  name: check-email-unavailable
  type: http
  seq: 3
}

get {
  url: {{base_url}}/auth/check-email?email={{testEmail}}
  body: none
  auth: none
}

tests {
  test("Status should be 200", function () {
    expect(res.getStatus()).to.equal(200);
  });

  test("Email should not be available", function () {
    expect(res.getBody().available).to.be.false;
  });
}
